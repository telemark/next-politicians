{
  "name": "politikere",
  "version": "1.0.0",
  "description": "Portal frontpage for Telemark fylkeskommune",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Jonas Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10.14.2"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "deploy": "next build && next export && now --public --static out/ --name politikere",
    "export": "rm -rf out && next build && next export",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "Politikere"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/portalen.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/portalen/issues"
  },
  "homepage": "https://github.com/telemark/portalen#readme",
  "devDependencies": {
    "ava": "1.4.1",
    "babel-eslint": "10.0.1",
    "coveralls": "3.0.3",
    "micro-dev": "3.0.0",
    "nyc": "15.1.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "axios": "0.19.0",
    "dotenv": "8.0.0",
    "next": "8.1.0",
    "next-routes": "1.4.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "now": {
    "name": "politkere",
    "alias": "www.politikere.win",
    "type": "npm",
    "engines": {
      "node": ">=10.0.0"
    },
    "dotenv": "production.env"
  }
}
